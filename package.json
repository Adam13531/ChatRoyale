{
  "name": "yata",
  "version": "1.10.0",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "prestart": "yarn run changelog",
    "start": "react-scripts-ts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "yarn run test --coverage && open coverage/index.html",
    "lint": "yarn run lint:ts; yarn run lint:css",
    "lint:ts": "tslint -p . -t stylish && tsc -p tsconfig.prod.json  --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged",
    "deploy": "now"
  },
  "dependencies": {
    "@blueprintjs/core": "3.18.0",
    "@blueprintjs/icons": "3.10.0",
    "@blueprintjs/select": "3.10.0",
    "animejs": "3.1.0",
    "bowser": "2.5.3",
    "clsx": "1.0.4",
    "copy-to-clipboard": "3.2.0",
    "file-saver": "2.0.2",
    "linkifyjs": "2.1.8",
    "localforage": "1.7.3",
    "lodash": "4.17.15",
    "marked": "0.7.0",
    "node-jose": "1.1.3",
    "pluralize": "8.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-flip-toolkit": "7.0.2",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "react-scripts-ts": "3.1.0",
    "react-timeago": "4.4.0",
    "react-tooltip": "3.10.0",
    "react-transition-group": "4.2.2",
    "react-virtualized": "9.21.1",
    "recompose": "0.30.0",
    "redux": "4.0.4",
    "redux-persist": "5.10.0",
    "reselect": "4.0.0",
    "semver-compare": "1.0.0",
    "shortid": "2.2.14",
    "styled-components": "4.3.2",
    "tinycolor2": "1.4.1",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "1.9.0",
    "@types/animejs": "3.1.0",
    "@types/file-saver": "2.0.1",
    "@types/jest": "24.0.17",
    "@types/lodash": "4.14.137",
    "@types/marked": "0.6.5",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "@types/react-helmet": "5.0.9",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.4",
    "@types/react-tooltip": "3.9.3",
    "@types/react-transition-group": "4.2.2",
    "@types/react-virtualized": "9.21.4",
    "@types/recompose": "0.30.7",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "4.0.3",
    "@types/tinycolor2": "1.4.2",
    "@types/webpack-env": "1.14.0",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "now": "16.1.2",
    "patch-package": "6.1.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "source-map-explorer": "2.0.1",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "tslint": "5.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "@types/react": "16.9.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
