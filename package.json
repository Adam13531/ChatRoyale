{
  "name": "yata",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "2.3.1",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.0.8",
    "linkifyjs": "2.1.6",
    "localforage": "1.7.2",
    "lodash": "4.17.10",
    "marked": "0.4.0",
    "node-jose": "1.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-tooltip": "3.6.1",
    "react-transition-group": "2.3.1",
    "react-virtualized": "9.19.1",
    "redux": "4.0.0",
    "redux-persist": "5.10.0",
    "reselect": "3.0.1",
    "semver-compare": "1.0.0",
    "shortid": "2.2.8",
    "styled-components": "3.3.2",
    "twitch-js": "1.2.12"
  },
  "scripts": {
    "prestart": "yarn run changelog",
    "start": "react-scripts-ts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "yarn run test --coverage && open coverage/index.html",
    "lint": "yarn run lint:ts; yarn run lint:css",
    "lint:ts": "tslint -p . -t stylish && tsc -p tsconfig.prod.json  --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "changelog": "cp CHANGELOG.md src",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@types/classnames": "2.2.4",
    "@types/jest": "23.1.1",
    "@types/lodash": "4.14.110",
    "@types/marked": "0.4.0",
    "@types/node": "10.3.4",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "@types/react-helmet": "5.0.6",
    "@types/react-redux": "6.0.2",
    "@types/react-router-dom": "4.2.7",
    "@types/react-tooltip": "3.3.5",
    "@types/react-transition-group": "2.0.11",
    "@types/react-virtualized": "9.18.3",
    "@types/recompose": "0.26.1",
    "@types/shortid": "0.0.29",
    "@types/webpack-env": "1.13.6",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.5",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.1",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.9.2"
  },
  "resolutions": {
    "@types/react": "16.4.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
