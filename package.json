{
  "name": "yata",
  "version": "1.2.2",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "yarn run changelog",
    "start": "react-scripts-ts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "yarn run test --coverage && open coverage/index.html",
    "lint": "yarn run lint:ts; yarn run lint:css",
    "lint:ts": "tslint -p . -t stylish && tsc -p tsconfig.prod.json  --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged",
    "deploy": "now --public",
    "now-start": "serve"
  },
  "dependencies": {
    "@blueprintjs/core": "3.3.0",
    "@blueprintjs/icons": "3.0.0",
    "@blueprintjs/select": "3.1.0",
    "animejs": "2.2.0",
    "bowser": "2.0.0-alpha.4",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.0.8",
    "file-saver": "1.3.8",
    "linkifyjs": "2.1.6",
    "localforage": "1.7.2",
    "lodash": "4.17.10",
    "marked": "0.4.0",
    "node-jose": "1.0.0",
    "pluralize": "7.0.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-flip-toolkit": "4.0.2",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.17.0",
    "react-timeago": "4.1.9",
    "react-tooltip": "3.6.1",
    "react-transition-group": "2.4.0",
    "react-virtualized": "9.20.1",
    "recompose": "0.28.2",
    "redux": "4.0.0",
    "redux-persist": "5.10.0",
    "reselect": "3.0.1",
    "semver-compare": "1.0.0",
    "shortid": "2.2.13",
    "styled-components": "3.4.2",
    "tinycolor2": "1.4.1",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "1.5.0",
    "@types/animejs": "2.0.0",
    "@types/classnames": "2.2.6",
    "@types/file-saver": "1.3.0",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/marked": "0.4.0",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.4.11",
    "@types/react-dom": "16.0.7",
    "@types/react-helmet": "5.0.7",
    "@types/react-redux": "6.0.6",
    "@types/react-router-dom": "4.3.0",
    "@types/react-tooltip": "3.3.6",
    "@types/react-transition-group": "2.0.13",
    "@types/react-virtualized": "9.18.6",
    "@types/recompose": "0.26.4",
    "@types/shortid": "0.0.29",
    "@types/tinycolor2": "1.4.1",
    "@types/webpack-env": "1.13.6",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "now": "11.3.10",
    "prettier": "1.14.2",
    "serve": "9.6.0",
    "source-map-explorer": "1.6.0",
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "tslint": "5.11.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "3.0.1"
  },
  "resolutions": {
    "@types/react": "16.4.11"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/YaTA/issues"
  },
  "contributors": [],
  "homepage": "https://yata.now.sh"
}
