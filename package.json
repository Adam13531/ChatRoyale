{
  "name": "yata",
  "version": "1.13.3",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "prestart": "yarn run changelog",
    "start": "react-scripts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts build",
    "lint": "yarn run lint:ts; yarn run lint:css",
    "lint:ts": "eslint --max-warnings=0 --ext ts,tsx src && tsc --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "precommit": "lint-staged",
    "deploy": "now"
  },
  "dependencies": {
    "@blueprintjs/core": "3.24.0",
    "@blueprintjs/icons": "3.16.0",
    "@blueprintjs/select": "3.12.3",
    "animejs": "3.2.0",
    "bowser": "2.9.0",
    "clsx": "1.1.0",
    "copy-to-clipboard": "3.3.1",
    "file-saver": "2.0.2",
    "linkifyjs": "2.1.9",
    "localforage": "1.7.3",
    "lodash": "4.17.15",
    "marked": "1.0.0",
    "node-jose": "1.1.4",
    "pluralize": "8.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-flip-toolkit": "7.0.9",
    "react-helmet-async": "1.0.6",
    "react-progressive-image": "0.6.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-timeago": "4.4.0",
    "react-tooltip": "4.2.6",
    "react-transition-group": "4.4.1",
    "react-virtualized": "9.21.2",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "semver-compare": "1.0.0",
    "shortid": "2.2.15",
    "styled-components": "5.1.0",
    "tinycolor2": "1.4.1",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@types/animejs": "3.1.0",
    "@types/file-saver": "2.0.1",
    "@types/lodash": "4.14.150",
    "@types/marked": "0.7.4",
    "@types/node": "13.13.5",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-transition-group": "4.2.4",
    "@types/react-virtualized": "9.21.9",
    "@types/recompose": "0.30.7",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "5.1.0",
    "@types/tinycolor2": "1.4.2",
    "@types/webpack-env": "1.15.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "now": "19.0.0",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.0.5",
    "source-map-explorer": "2.4.2",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "@types/react": "16.9.35"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/YaTA/issues"
  },
  "contributors": [
    "nD00rn <ndoorn@outlook.com>",
