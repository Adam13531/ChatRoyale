{
  "name": "yata",
  "version": "1.13.1",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "prestart": "yarn run changelog",
    "start": "react-scripts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts build",
    "lint": "yarn run lint:ts; yarn run lint:css",
    "lint:ts": "eslint --max-warnings=0 --ext ts,tsx src && tsc --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "precommit": "lint-staged",
    "deploy": "now"
  },
  "dependencies": {
    "@blueprintjs/core": "3.22.2",
    "@blueprintjs/icons": "3.12.0",
    "@blueprintjs/select": "3.11.2",
    "animejs": "3.1.0",
    "bowser": "2.7.0",
    "clsx": "1.0.4",
    "copy-to-clipboard": "3.2.0",
    "file-saver": "2.0.2",
    "linkifyjs": "2.1.8",
    "localforage": "1.7.3",
    "lodash": "4.17.15",
    "marked": "0.8.0",
    "node-jose": "1.1.3",
    "pluralize": "8.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-flip-toolkit": "7.0.7",
    "react-helmet-async": "1.0.4",
    "react-progressive-image": "0.6.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "react-timeago": "4.4.0",
    "react-tooltip": "3.11.1",
    "react-transition-group": "4.3.0",
    "react-virtualized": "9.21.2",
    "recompose": "0.30.0",
    "redux": "4.0.4",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "semver-compare": "1.0.0",
    "shortid": "2.2.15",
    "styled-components": "4.4.1",
    "tinycolor2": "1.4.1",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@types/animejs": "3.1.0",
    "@types/file-saver": "2.0.1",
    "@types/lodash": "4.14.149",
    "@types/marked": "0.7.2",
    "@types/node": "12.12.17",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.3",
    "@types/react-router-dom": "5.1.3",
    "@types/react-tooltip": "3.11.0",
    "@types/react-transition-group": "4.2.3",
    "@types/react-virtualized": "9.21.6",
    "@types/recompose": "0.30.7",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "4.4.1",
    "@types/tinycolor2": "1.4.2",
    "@types/webpack-env": "1.14.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "now": "16.7.1",
    "patch-package": "6.2.0",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.19.1",
    "source-map-explorer": "2.1.2",
    "stylelint": "12.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0",
    "typescript": "3.7.3"
  },
  "resolutions": {
    "@types/react": "16.9.16"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": { "node": "12.x" },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/YaTA/issues"
  },
  "contributors": [
    "nD00rn <ndoorn@outlook.com>",
    "tollus"
  ],
  "homepage": "https://yata.now.sh"
