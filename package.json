{
  "name": "yata",
  "version": "1.15.0",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "prestart": "yarn run changelog",
    "start": "react-scripts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts build",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:ts": "eslint --max-warnings=0 --ext ts,tsx src && tsc --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "precommit": "lint-staged",
    "deploy": "now"
  },
  "dependencies": {
    "@blueprintjs/core": "3.36.0",
    "@blueprintjs/icons": "3.23.0",
    "@blueprintjs/select": "3.15.0",
    "animejs": "3.2.1",
    "bowser": "2.11.0",
    "clsx": "1.1.1",
    "copy-to-clipboard": "3.3.1",
    "file-saver": "2.0.5",
    "linkifyjs": "2.1.9",
    "localforage": "1.9.0",
    "lodash": "4.17.20",
    "marked": "1.2.7",
    "node-jose": "2.0.0",
    "pluralize": "8.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-flip-toolkit": "7.0.13",
    "react-helmet-async": "1.0.7",
    "react-progressive-image": "0.6.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-timeago": "5.2.0",
    "react-tooltip": "4.2.10",
    "react-transition-group": "4.4.1",
    "react-virtualized": "9.22.3",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "semver-compare": "1.0.0",
    "shortid": "2.2.15",
    "styled-components": "5.2.1",
    "tinycolor2": "1.4.2",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@types/animejs": "3.1.2",
    "@types/file-saver": "2.0.1",
    "@types/lodash": "4.14.165",
    "@types/marked": "1.2.1",
    "@types/node": "14.14.14",
    "@types/pluralize": "0.0.29",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router-dom": "5.1.6",
    "@types/react-transition-group": "4.4.0",
    "@types/react-virtualized": "9.21.10",
    "@types/recompose": "0.30.7",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "5.1.7",
    "@types/tinycolor2": "1.4.2",
    "@types/webpack-env": "1.16.0",
    "eslint-plugin-prettier": "3.3.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "now": "21.0.1",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.2.1",
    "source-map-explorer": "2.5.1",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.1.3"
  },
  "resolutions": {
    "@types/react": "17.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/no-anonymous-default-export": [
        "warn",
        {
          "allowObject": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "chrome 79",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/YaTA/issues"
  },
  "contributors": [
    "nD00rn <ndoorn@outlook.com>",
    "tollus"
  ],
  "homepage": "https://yata.now.sh"
}
