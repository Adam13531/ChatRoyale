{
  "directoryListing": false,
  "headers": [
    {
      "source": "static/**/*.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ],
  "public": "build",
  "rewrites": [{ "source": "**", "destination": "/index.html" }]
}
